    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Example",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "build example",
            "program": "${workspaceFolder}/examples/hello/dist/app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "cwd": "${workspaceFolder}/examples/hello",
            "envFile": "${workspaceFolder}/examples/hello/.env"
        }
    ]
}