{
  "name": "@dbos-inc/dbos-sdk",
  "version": "0.0.0-placeholder",
  "description": "A Typescript framework built on the database",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbos-inc/dbos-sdk"
  },
  "homepage": "https://docs.dbos.dev/",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && chmod +x ./dist/src/dbos-runtime/cli.js",
    "build-fdb": "tsc --project tsconfig.build-fdb.json && chmod +x ./dist/src/dbos-runtime/cli.js",
    "lint": "eslint --fix --ext .ts --ignore-pattern foundationdb/ .",
    "lint-fdb": "eslint --fix --ext .ts .",
    "setversion": "grunt setversion",
    "test": "npx prisma generate --schema tests/prisma/schema.prisma && npm run build && jest --coverage --collectCoverageFrom='src/**/*' --collectCoverageFrom='!src/foundationdb/**/*' --detectOpenHandles --testPathIgnorePatterns=examples/* --testPathIgnorePatterns=tests/foundationdb/* --testPathIgnorePatterns=tests/provenance/*",
    "test-fdb": "npx prisma generate --schema tests/prisma/schema.prisma && npm run build-fdb && jest --runInBand --coverage"
  },
  "bin": {
    "dbos": "./dist/src/dbos-runtime/cli.js",
    "dbos-cloud": "./dist/src/cloud-cli/cli.js"
  },
  "devDependencies": {
    "@prisma/client": "^5.3.1",
    "@types/jest": "^29.5.3",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/koa-logger": "^3.1.2",
    "@types/node": "^20.6.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "grunt": "^1.6.1",
    "jest": "^29.6.1",
    "nerdbank-gitversioning": "^3.6.133",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typeorm": "^0.3.17"
  },
  "dependencies": {
    "@koa/bodyparser": "^5.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.2",
    "@opentelemetry/sdk-trace-base": "^1.15.2",
    "@types/koa": "^2.13.8",
    "@types/lodash": "^4.14.197",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "fast-glob": "^3.3.1",
    "form-data": "^4.0.0",
    "jwt-simple": "^0.5.6",
    "knex": "^3.0.1",
    "koa": "^2.14.2",
    "koa-logger": "^3.2.1",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.3",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "8.1.0",
    "ts-json-schema-generator": "^1.4.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "yaml": "^2.3.1"
  },
  "optionalDependencies": {
    "foundationdb": "^2.0.1"
  }
}
